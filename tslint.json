{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "quotemark": [true, "single", "avoid-escape"],
        "max-line-length": [true, 120],
        "no-unused-variable": [true],
        "no-var-keyword": true,
        "ordered-imports": [false],
        "newline-before-return": true
    }
}
